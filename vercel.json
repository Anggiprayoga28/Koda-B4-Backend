{
  "version": 2,
  "builds": [
    {
      "src": "api/entrypoint.go",
      "use": "@vercel/go@3.1.0"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/entrypoint.go"
    }
  ],
  "functions": {
    "api/entrypoint.go": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}